#INITIAL SETUP
git init
git add README.md
git commit -m "first commit"
git branch -M main
git remote add origin https://github.com/reaxster/postgram.git
git push -u origin main

#CLONE
git clone https://github.com/reaxster/postgram.git

#PULL
git pull https://github.com/reaxster/postgram.git

#PUSHING
git status #Archivos no Salvados
git add . #AÃ±ade todos los Archivos para salvar
git commit -m "Mensage del cambio que se hiso"
git push -u origin yourbranch

#GIT UTIL
git branch #muentra el branck que estas usando
git branch branchname #creas un branch nuevo
git switch branchname #cambia el branch que estas usando

